//C++ Program To Calculate Factorial.



#include<bits/stdc++.h>
using namespace std;
int main()
{
    long number, factorial=1;
    cout<<"Enter a Positive Integer\n";
    cin>>number;

    for(int i=1; i<=number; i++)
    {
        factorial = factorial*i;
    }

    cout<<"Factorial of "<<number<<" is = "<<factorial;

    return 0;
}
